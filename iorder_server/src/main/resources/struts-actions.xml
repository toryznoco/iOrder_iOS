<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="iorder-actions" extends="iorder-default"
		namespace="/server">
		
		<action name="shop" class="cn.net.normcore.iorder.action.ShopAction">
			<result name="shops" type="json">
				<param name="root">shops</param>
			</result>
			
			<result name="shopDishes" type="json">
				<param name="root">shopDishes</param>
			</result>
		</action>
		
	</package>

</struts>